package sp0424.bean;

public class HelloBean {

	private String name;
	//호출횟수 출력용 객체
	private MyLogger logger;
	
	public String sayHello() {
		return "Hello" + (name == null?"":" "+name);
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

}
