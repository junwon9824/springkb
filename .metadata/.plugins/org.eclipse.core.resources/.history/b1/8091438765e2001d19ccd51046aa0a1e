package sp0424.bean;

public class NumberGuess {

	private int answer;

	public NumberGuess() {
	}

	public NumberGuess(int answer) {
		super();
		this.answer = answer;
	}

	public String checkNum(int userNum) {
		if (userNum > answer)
			return "撤苗林技夸";
		if (userNum < answer)
			return "臭咯林技夸";
	}

}
